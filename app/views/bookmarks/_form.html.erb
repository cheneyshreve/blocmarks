<%= form_for [topic,bookmark] do |f| %>
   <div class="form-group">
     <%= f.label :url %>
     <%= f.text_field :url, class: 'form-control', placeholder: "Enter bookmark url here" %>
   </div>

   <div class="form-group">
       <%= f.submit "Save", class: 'btn btn-success' %>
   </div>
 <% end %>
